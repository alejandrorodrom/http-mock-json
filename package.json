  "dependencies": {
    "colorette": "^2.0.17",
    "commander": "^9.3.0",
    "express": "^4.18.1",
    "figlet": "^1.5.2",
    "sisteransi": "^1.0.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/figlet": "^1.5.4",
    "@types/node": "^18.6.0",
    "typescript": "^4.7.4"
  }
